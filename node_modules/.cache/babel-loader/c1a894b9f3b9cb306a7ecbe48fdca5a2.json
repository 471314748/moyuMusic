AA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,MAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KARA,EAbA,CAsBA;;AACA,IAAA,KAAA,CAAA;AACA,MAAA,GAAA,EAAA,6BADA;AAEA,MAAA,MAAA,EAAA;AACA,QAAA,EAAA,EAAA;AADA;AAFA,KAAA,CAAA,CAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,MAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,GAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA;AACA,KARA;AASA;AAlDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"player\" v-if=\"songInfo\">\r\n    <div class=\"left\">\r\n      <img class=\"disc\" src=\"../assets/img/disc.png\" alt />\r\n      <img class=\"cover\" :src=\"songInfo.al.picUrl\" alt />\r\n    </div>\r\n    <div class=\"right\">\r\n      <div class=\"title\">\r\n        <img src=\"../assets/img/tag.png\" alt />\r\n        <span>{{songInfo.name}}</span>\r\n      </div>\r\n      <div class=\"singer\">\r\n        歌手:\r\n        <span>{{songInfo.ar | filterNames}}</span>\r\n      </div>\r\n      <div class=\"album\">\r\n        所属专辑:\r\n        <span>{{songInfo.al.name}}</span>\r\n      </div>\r\n      <audio class=\"audio\" controls :src='songUrl'></audio>\r\n      <ul class=\"lyric-container\">\r\n        <li class=\"lyric\" v-for=\"(item, index) in lyricArr\" :key=\"index\">\r\n          {{item.split(']')[1]}}\r\n          </li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n    data() {\r\n        return {\r\n            songUrl:'',\r\n            songInfo:'',\r\n            lyricArr:[],\r\n        }\r\n    },\r\n    filters:{\r\n      filterNames(singerArr) {\r\n      let newArr = singerArr.map(ele => {\r\n        return ele.name;\r\n      });\r\n      return newArr.join(\" | \");\r\n    },\r\n    },\r\n\r\n    \r\n    created() {\r\n        let _id=this.$route.query.id;\r\n        // 音频\r\n        axios({\r\n            url:'https://autumnfish.cn/song/url',\r\n            params:{\r\n                id:_id\r\n            }\r\n        }).then(res=>{\r\n            console.log(res);\r\n            this.songUrl=res.data.data[0].url;\r\n        });\r\n        // 歌曲信息\r\n        axios({\r\n            url:'https://autumnfish.cn/song/detail',\r\n            params:{\r\n                ids:_id\r\n            }\r\n        }).then(res=>{\r\n            console.log(res);\r\n            this.songInfo=res.data.songs[0];\r\n        });\r\n        // 歌词\r\n        axios({\r\n            url:'https://autumnfish.cn/lyric',\r\n            params:{\r\n                id:_id\r\n            }\r\n        }).then(res=>{\r\n            console.log(res);\r\n            this.lyricArr=res.data.lrc.lyric.split('\\n');\r\n        });\r\n    },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/components"}]}